download() {
    :
}

build() {
    $CC $CFLAGS `pkg-config --cflags libxxhash` update-adlist.c $LDFLAGS `pkg-config --libs libxxhash` -o /usr/local/sbin/update-adlist
    $CC $CFLAGS `pkg-config --cflags libxxhash` nss-adlist.c $LDFLAGS `pkg-config --libs libxxhash` -shared -o /usr/local/lib/libnss_adlist.so.2
}
