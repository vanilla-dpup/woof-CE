download() {
    :
}

build() {
    $CC $CFLAGS -D_GNU_SOURCE pkexec.c $LDFLAGS -o /usr/local/bin/pkexec
    $CC $CFLAGS -D_GNU_SOURCE pkexecd.c $LDFLAGS -o /usr/local/bin/pkexecd
    if [ -e /usr/include/linux/landlock.h ]; then
        $CC $CFLAGS -D_GNU_SOURCE -DHAVE_LANDLOCK sandbox.c $LDFLAGS -o /usr/local/bin/spot-sandbox
    else
        $CC $CFLAGS -D_GNU_SOURCE sandbox.c $LDFLAGS -o /usr/local/bin/spot-sandbox
    fi
}