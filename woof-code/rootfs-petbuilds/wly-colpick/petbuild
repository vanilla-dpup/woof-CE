download() {
    [ -f wly-colpick-0.1.1.tar.gz ] || curl --fail --location --retry 3 --max-time 60 -o wly-colpick-0.1.1.tar.gz https://github.com/01micko/wly-colpick/archive/refs/tags/wly-colpick-0.1.1.tar.gz
}

build() {
    tar -xzf wly-colpick-0.1.1.tar.gz
    cd wly-colpick-wly-colpick-0.1.1
    make install
}
