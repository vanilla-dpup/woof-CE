download() {
    [ -f gexec-0.5.1.tar.gz ] || curl --fail --location --retry 3 --max-time 60 -o gexec-0.5.1.tar.gz https://github.com/puppylinux-woof-CE/gexec/archive/refs/tags/0.5.1.tar.gz
}

build() {
    tar -xzf gexec-0.5.1.tar.gz
    cd gexec-0.5.1
    make
    install -m 755 gexec /usr/bin/gexec
}