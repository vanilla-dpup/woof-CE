#!/bin/ash

if [ "$1" = "start" ]; then
	[ -x /etc/init.d/rc.firewall ] && /etc/init.d/rc.firewall start
	[ -x /etc/init.d/trim ] && /etc/init.d/trim start
fi

for S in /etc/rc5.d/*
do
	case "${S##*/}" in
	README|*dbus|*udev|*elogind|*bootlogs|*rc.local|*sendsigs|*syslog*|*busybox-klogd) ;;
	*) [ -x "$S" ] && $S $1 ;;
	esac
done
